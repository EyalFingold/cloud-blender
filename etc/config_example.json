{
   "identitySettingsHPCS": {
      "auth": {
         "apiAccessKeyCredentials": {
            "accessKey": "<your hp access key here>",
            "secretKey": "<your hp secret key here>"
         },
         "tenantId": "<your tenant id>"
      },
      "url": "https://region-a.geo-1.identity.hpcloudsvc.com:35357/v2.0/tokens" //geo 1 in hpcs is uswest
   },
   "computeSettingsHPCS": {
      "url": "https://az-2.region-a.geo-1.compute.hpcloudsvc.com/v1.1/<your tenant id>" // az2 in us west of the given tenant id
   },
   "keyNameHPCS": "<your key name>",
   "imageIdHPCS": "<image id>",

   "identitySettingsAWS": {
      "credentials": { 
         "accessKeyId": "<your aws access key here>",
         "secretAccessKey": "<your aws secret key here>"
      }
   },
   "computeSettingsAWS": {
      "region": "us-east-1"
   },
   "keyNameAWS": "<your key name>",
   "imageIdAWS": "<ami id>",

   "tunnelingProxyURL": "<proxy url>" // delete this line if you don't need a proxy
}
